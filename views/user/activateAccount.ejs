<% include ../partials/header.ejs%>

<div class="container">
<div class="row">
	<div class="col-md-12 col-lg-4">
		<form action="/register %>" method="POST">
		  <legend>Set Your Password For AceAtKart Account</legend>
		  <div class="form-group">
		    <label for="password">Password</label>
		    <input type="password" name="password"  placeholder="Enter password" autofocus="autofocus" class="form-control"/>
		  </div>
		  <div class="form-group">
		    <label for="confirm">Confirm Password</label>
		    <input type="password" name="confirm"  placeholder="Confirm password" class="form-control"/>
		  </div>
		  
		  <div class="form-group">
		    <button type="submit" class="btn btn-primary">Submit</button>
		  </div>
		  
		  
		  
		   <div class="form-group">
                  
                  <div class="col-lg-4 col-md-12 ">
                    <input type="hidden" class="form-control" id="name" value="<%=data.name%>" name="name" >
                  </div>
                </div>
              
              <div class="form-group">
                 
                  <div class="col-lg-4 col-md-12 ">
                    <input type="hidden" class="form-control" id="username"  name="username" value="<%=data.username%>">
                  </div>
                </div>
                <div class="form-group">
                 
                  <div class="col-lg-4 col-md-12 ">
                    <input type="hidden" class="form-control" id="name"  name="phone" value="<%=data.phone%>">
                  </div>
                </div>
                <div class="form-group">
                
                  <div class="col-lg-4 col-md-12 ">
                    <input type="hidden" class="form-control" id="email"  name="email" value="<%=data.email%>">
                  </div>
                </div>
                
                <div class="form-group">
                  
                  <div class="col-lg-4 col-md-12 ">
                  <input type="hidden" class="form-control" id="address" value="<%=data.address%>" name="address">
                  </div>
                </div>
              
              <div class="form-group">
                  
                  <div class="col-lg-4 col-md-12 ">
                    <input type="hidden" class="form-control" id="city"  name="city" value="<%=data.city%>">
                  </div>
                </div>
                
                <div class="form-group">
                  
                  <div class="col-lg-4 col-md-12 ">
                    <input type="hidden" class="form-control" id="country" value="<%=data.country%>" name="country">
                  </div>
                </div>
		  
		  
		  
		  
		</form>
	</div>
</div>	
	
</div>

<% include ../partials/footer.ejs%>

